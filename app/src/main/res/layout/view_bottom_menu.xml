<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <include
        android:id="@+id/layoutNormal"
        layout="@layout/menu_normal"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutHandler"
        layout="@layout/menu_handler"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutRecycleBin"
        layout="@layout/menu_recycle_bin"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutApk"
        layout="@layout/menu_app"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutAccount"
        layout="@layout/menu_account"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutMenuExtract"
        layout="@layout/menu_extract"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutMoveTo"
        layout="@layout/menu_move_to"
        android:layout_width="@dimen/size_0"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutMenuDropBox"
        layout="@layout/menu_drop_box"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutMenuAlbum"
        layout="@layout/menu_album"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layoutMenuHide"
        layout="@layout/menu_hide_file"
        android:layout_width="@dimen/size_0"
        android:layout_height="@dimen/size_70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="layoutNormal, layoutHandler, layoutRecycleBin, layoutApk, layoutAccount, layoutMenuExtract, layoutMoveTo, layoutMenuDropBox, layoutMenuAlbum,layoutMenuHide" />

</androidx.constraintlayout.widget.ConstraintLayout>
